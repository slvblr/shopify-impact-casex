 display: flex;\n  align-items: center;\n  gap: 10px;\n  font-size: 18px;\n  font-weight: bold;\n  color: #333;\n}\n\n.delivery-header img {\n  width: 24px;\n  height: 24px;\n}\n\n/* Pincode Section */\n.pincode-section {\n  display: flex;\n  gap: 10px;\n  align-items: center;\n}\n\n.pincode-section input {\n  width: 70%;\n  padding: 10px;\n  border: 1px solid #ccc;\n  border-radius: 8px;\n  font-size: 14px;\n}\n\n.pincode-section button {\n  background: #ff4081;\n  color: #fff;\n  border: none;\n  padding: 10px 20px;\n  cursor: pointer;\n  border-radius: 8px;\n  font-size: 14px;\n}\n\n.pincode-section button:hover {\n  background: #e91e63;\n}\n\n/* Pincode Message */\n#pincode-message {\n  color: #777;\n  font-size: 14px;\n  margin-top: 10px;\n}\n\n/* Results Section */\n#delivery-results {\n  display: none;\n  margin-top: 20px;\n}\n\n.pincode-display {\n  display: flex;\n  align-items: center;\n  gap: 10px;\n  background: #f4f4f4;\n  padding: 10px;\n  border-radius: 8px;\n}\n\n#entered-pincode {\n  border: none;\n  background: transparent;\n  font-size: 16px;\n  font-weight: bold;\n}\n\n#success-icon {\n  color: #4CAF50;\n  font-size: 18px;\n}\n\n#change-btn {\n  background: transparent;\n  color: #ff4081;\n  border: none;\n  cursor: pointer;\n  font-weight: bold;\n}\n\n/* Delivery Info Section */\n.delivery-info {\n  margin-top: 20px;\n  display: flex;\n  flex-direction: column;\n  gap: 15px;\n}\n\n.delivery-item {\n  display: flex;\n  align-items: center;\n  gap: 15px;\n  padding: 12px;\n  background: #f9f9f9;\n  border-radius: 8px;\n  box-shadow: 0 2px 8px rgba(0, 0, 0, 0.1);\n}\n\n.delivery-item img {\n  width: 40px;\n  height: 40px;\n}\n\n.delivery-item p {\n  margin: 0;\n  font-size: 14px;\n  color: #333;\n  font-weight: bold;\n}\n\n/* Responsive */\n@media (max-width: 768px) {\n  .delivery-options-container {\n    padding: 15px;\n  }\n\n  .pincode-section input {\n    width: 60%;\n  }\n\n  .delivery-item img {\n    width: 30px;\n    height: 30px;\n  }\n\n  